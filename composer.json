{
    "name": "timostamm/json-client",
    "description": "A simple client for JSON APIs using Guzzle and the Symfony Serializer.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.5 || ^7.4",
        "symfony/serializer": "^5.4 || ^6.4",
        "symfony/options-resolver": "^5.4 || ^6.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Timo Stamm",
            "email": "ts@timostamm.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "TS\\Web\\JsonClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TS\\Web\\JsonClient\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.20",
        "psr/log": "^3.0"
    },
    "conflict": {
        "guzzlehttp/guzzle": "<6.5.8",
        "psr/http-message": "<1.1",
        "guzzlehttp/promises": "<1.4"
    }
}
